{
  "scripts": {
    "test": "jest --coverage --reporters=default --reporters=jest-html-reporter --testEnvironment jsdom"
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "./setup-jest.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/app/**",
      "!**/assets/**",
      "!**/external/**",
      "!**/fixtures/**",
      "!**/lcov-report/**"
    ],
    "testEnvironment": "jsdom",
    "preset": "jest-puppeteer"
  },
  "jest-puppeteer": {},
  "type": "module",
  "dependencies": {
    "express": "^4.17.1",
    "global": "^4.4.0",
    "jquery": "^3.7.1",
    "path": "^0.12.7",
    "selenium-webdriver": "^4.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/user-event": "^12.0.11",
    "babel-jest": "^26.1.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^27.4.6",
    "jest-html-reporter": "^3.1.3",
    "jest-html-reporters": "^3.1.7",
    "jest-puppeteer": "^10.0.1",
    "puppeteer": "^22.1.0"
  }
}
